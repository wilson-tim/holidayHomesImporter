      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2849A273-58D9-4244-8B9D-6550A2BF4A21}"
          SQLTask:SqlStatementSource="INSERT INTO tab_PropertyRaw&#xA;  (PropertySourceID,&#xA;  BatchID,&#xA;  Custref1,&#xA;  CompletionDate,&#xA;  Comments,&#xA;  Bathrooms,&#xA;  Bedrooms,&#xA;  GarageSpaces,&#xA;  Garage,&#xA;  Kitchens,&#xA;  Outbuildings,&#xA;  Receptions,&#xA;  ToiletRooms,&#xA;  ConcatenatedAddress,&#xA;  HouseName,&#xA;  StreetName,&#xA;  Town,&#xA;  County,&#xA;  Postcode,&#xA;  FloorArea,&#xA;  Condition,&#xA;  ConstrType,&#xA;  EntranceFloor,&#xA;  FloorsInBlock,&#xA;  AssessedRent,&#xA;  YearBuilt,&#xA;  Heating,&#xA;  Tenure,&#xA;  ValAfter,&#xA;  ValPresent,&#xA;  PropType,&#xA;  PropStyle,&#xA;  ReportDate,&#xA;  InspectionDate)&#xA;SELECT  PropertySourceID,&#xA;  BatchID,&#xA;  LEFT(a_Case_ID, 50),&#xA;  LEFT(a_Date_Completed, 25),&#xA;  comments = LEFT('Attic_rooms= ' + a_AccomAttic + ',Cellars=' + a_AccomCellars +&#xA;     CASE&#xA;      WHEN ISNUMERIC(a_OtherPotentialIncomefurnished) = 1&#xA;      THEN ',RentalType=Furnished'&#xA;      WHEN a_OtherPotentialIncomefurnished IS NOT NULL&#xA;      THEN ',RentalType=Unfurnished'&#xA;      ELSE ''&#xA;      END, 255),&#xA;  LEFT(a_AccomBathroom,10),&#xA;  LEFT(a_AccomBedrooms,10),&#xA;  LEFT(a_AccomGarages,50),&#xA;  LEFT(a_AccomGaragesPresent, 10),&#xA;  LEFT(a_AccomKitchen,10),&#xA;  LEFT(a_AccomOutbuildings,10),&#xA;  LEFT(a_AccomReceptions,10),&#xA;  LEFT(a_AccomSeperateWC,10),&#xA;  ConcatenatedAddress = LEFT(LTRIM(ISNULL(a_ApplicantPropertyAddress_1 + ' ', '')) +&#xA;   LTRIM(ISNULL(a_ApplicantPropertyAddress_2 + ' ', '')) +&#xA;   LTRIM(ISNULL(a_ApplicantPropertyTown + ' ', '')) + &#xA;   LTRIM(ISNULL(a_ApplicantPropertyCounty + ' ', '')),511),&#xA;  LEFT(a_ApplicantPropertyAddress_1,100),&#xA;  LEFT(a_ApplicantPropertyAddress_2,100),&#xA;  LEFT(a_ApplicantPropertyTown,100),&#xA;  LEFT(a_ApplicantPropertyCounty,100),&#xA;  LEFT(a_ApplicantPropertyPostcode,10),&#xA;  LEFT(a_Area,10),&#xA;  LEFT(a_ConditionStructuralRepair,50),&#xA;  LEFT(a_ConstrWalltype + '/' + a_ConstrRoofType,25),&#xA;  LEFT(a_FlatFloorNumber,10),&#xA;  LEFT(a_FlatFloors,10),&#xA;  assessedRent = LEFT(CASE&#xA;      WHEN ISNUMERIC(a_OtherPotentialIncomefurnished) = 1&#xA;      THEN a_OtherPotentialIncomefurnished &#xA;      ELSE a_OtherPotentialIncomeUnfurnished&#xA;      END,20),&#xA;  LEFT(a_PropertyAge,10),&#xA;  LEFT(a_ServiceCentralHeatingType,50),&#xA;  LEFT(a_TenureType,50),&#xA;  LEFT(a_ValAfterWork,50),&#xA;  LEFT(a_ValCurrent,50),&#xA;  LEFT(a_ValPropertyType_1,25),&#xA;  LEFT(a_ValPropertyType_2,25),&#xA;  LEFT(a_ValuerReportDate,25),&#xA;  LEFT(a_ValuerValuationDate,25)&#xA;FROM  realtime_raw..imp_HSBC_raw&#xA;WHERE  Updated = 0&#xA;  AND BatchID = ?" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
