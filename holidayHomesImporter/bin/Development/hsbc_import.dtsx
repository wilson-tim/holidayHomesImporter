          <ProjectItem
            Name="ScriptMain.vb"
            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic
' The ScriptMain class is the entry point of the Script Task.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum

	' The execution engine calls this method when the task executes.
	' To access the object model, use the Dts object. Connections, variables, events,
	' and logging features are available as static members of the Dts class.
	' Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
	' 
	' To open Code and Text Editor Help, press F1.
	' To open Object Browser, press Ctrl+Alt+J.

    Public Sub Main()
        'Declare variables
        Dim objInFile As System.IO.File
        Dim objRead As System.IO.StreamReader
        Dim objWrite As System.IO.StreamWriter
        Dim strData As String, strChar As String, strDataOut As String, strError As String
        Dim intLocation As Integer, intDataLen As Integer
        Dim blnStartField As Boolean


        'Open input and output files
        objRead = objInFile.OpenText(Dts.Variables("fileFullPath").Value.ToString)
        Dts.Variables("fileFullPath").Value = Left(Dts.Variables("fileFullPath").Value.ToString, (Dts.Variables("fileFullPath").Value.ToString.Length - 4)) + "_clean.xml"
        objWrite = objInFile.CreateText(Dts.Variables("fileFullPath").Value.ToString)

        Do While objRead.EndOfStream = False
            strData = objRead.ReadLine.Trim
            intDataLen = strData.Length - 1
            If intDataLen > 1 Then
                If Left(strData, 5) = "<fld_" Then
                    blnStartField = True
                End If
                For intLocation = 0 To intDataLen
                    strChar = strData.Substring(intLocation, 1)
                    'MsgBox((intDataLen - 6), MsgBoxStyle.AbortRetryIgnore, "test")
                    'MsgBox(intLocation, MsgBoxStyle.AbortRetryIgnore, "test")
                    'MsgBox(blnStartField, MsgBoxStyle.AbortRetryIgnore, "test")
                    'MsgBox(strData.Substring(intLocation, 6), MsgBoxStyle.AbortRetryIgnore, "test")
                    'MsgBox(strDataOut, MsgBoxStyle.AbortRetryIgnore, "test")
                    If blnStartField And intLocation < (intDataLen - 6) Then
                        If strData.Substring(intLocation, 6) = "</fld_" Then
                            blnStartField = False
                        End If
                    End If
                    Try
                        If (Asc(strChar) < 123 And Asc(strChar) > 31) Or Asc(strChar) = 156 Then
                            strDataOut += strChar
                        End If
                    Finally
                        strError = ""
                    End Try
                Next
                If Not blnStartField Then
                    'MsgBox(blnStartField, MsgBoxStyle.AbortRetryIgnore, "test")
                    'MsgBox(strDataOut, MsgBoxStyle.AbortRetryIgnore, "test")
                    objWrite.WriteLine(strDataOut)
                    strDataOut = ""
                End If
            End If
        Loop

        'Close files
        objRead.Close()
        objWrite.Close()
        'Close objects
        objInFile = Nothing
        objRead = Nothing
        objWrite = Nothing

        'get results
        Dts.TaskResult = ScriptResults.Success
    End Sub

End Class]]></ProjectItem>
