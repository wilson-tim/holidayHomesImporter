					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<!-- Another URL for the property -->
				<xs:element minOccurs="0" name="detailsUrl">
				  <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<xs:element minOccurs="0" name="detailsUnit_size_units">
