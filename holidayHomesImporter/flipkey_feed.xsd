							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="photo_type" >
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
								</xs:restriction>
							</xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="width" type="xs:integer"/>
							<xs:element minOccurs="0" name="original_width" type="xs:integer"/>
							<xs:element minOccurs="0" name="order" type="xs:integer"/>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
				<!-- property_flags -->
                <xs:element minOccurs="0" name="flagsCalendar_export_url" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="flagsRates_updated" type="xs:dateTime" />
                <xs:element minOccurs="0" name="flagsLast_booked_date" type="xs:dateTime" />
				<!-- either 1 or 0 for -->
                <xs:element minOccurs="0" name="flagsHas_special" type="xs:boolean" />
                <xs:element minOccurs="0" name="flagsPhotos_updated" type="xs:dateTime" />
                <xs:element minOccurs="0" name="flagsIs_tip" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="flagsCalendar_updated" type="xs:dateTime" />
                <xs:element minOccurs="0" name="flagsAlt_photo_test" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="flagsAlt_pdp_sprite_filename_v2" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<!-- property_bedrooms -->
                <xs:element minOccurs="0" name="property_bedrooms" >
					<xs:complexType>
					  <xs:sequence>
						<xs:element minOccurs="0" maxOccurs="unbounded" name="property_bedroom">
							<xs:complexType>
							  <xs:sequence>
								  <xs:element name="twin_count" type="xs:integer"/>
								  <xs:element name="other_count" type="xs:integer"/>
								  <xs:element name="description" >
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="4000"/>
										</xs:restriction>
									</xs:simpleType>
								  </xs:element>
								  <xs:element name="standard_count" type="xs:integer"/>
								  <xs:element name="bunk_count" type="xs:integer"/>
								  <xs:element name="queen_count" type="xs:integer"/>
								  <xs:element name="king_count" type="xs:integer"/>
							  </xs:sequence>
							</xs:complexType>
						  </xs:element>
					  </xs:sequence>
					</xs:complexType>
				</xs:element>
				<!-- property_addresses -->
                <xs:element minOccurs="0" name="addressCity" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="addressLocation_name" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="addressZip" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="addressAddress1" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="addressAddress2" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="addressPrecision" >
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
                <xs:element minOccurs="0" name="addressLongitude" type="xs:decimal" />
