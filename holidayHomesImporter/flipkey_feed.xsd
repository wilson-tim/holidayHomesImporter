				<xs:element minOccurs="0" name="detailsPet">
				  <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<xs:element minOccurs="0" name="detailsUnit_size">
				  <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<!-- This should be one of the ISO 4217 Currency Codes -->
				<xs:element minOccurs="0" name="detailsCurrency">
				  <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<xs:element minOccurs="0" name="detailsHandicap">
