				  <xs:sequence>
					<xs:element minOccurs="0" maxOccurs="unbounded" name="booked_date" type="xs:date" />
				  </xs:sequence>
				</xs:complexType>
				</xs:element>
				<!-- review_count/review_sum gives star rating -->
                <xs:element minOccurs="0" name="review_sum" type="xs:integer" />
				<!-- property_amenities -->
                <xs:element minOccurs="0" name="property_amenities">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" maxOccurs="unbounded" name="property_amenity">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element minOccurs="0" name="description" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4000"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
                            <xs:element minOccurs="0" name="parent_amenity" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
                            <xs:element minOccurs="0" name="site_amenity" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
                            <xs:element minOccurs="0" name="order" type="xs:integer" />
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
				<!-- property_calendars -->
                <xs:element minOccurs="0" name="property_calendars">
