					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<xs:element minOccurs="0" name="detailsChildren">
				  <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<!-- property_bathrooms -->
				  <xs:element minOccurs="0" name="property_bathrooms">
					<xs:complexType>
					  <xs:sequence>
						<xs:element minOccurs="0" maxOccurs="unbounded" name="property_bathroom">
							<xs:complexType>
							  <xs:sequence>
								<xs:element minOccurs="0" name="type">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="50"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element minOccurs="0" name="description">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="4000"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							  </xs:sequence>
							</xs:complexType>
						</xs:element>
					  </xs:sequence>
					</xs:complexType>
				  </xs:element>
				<!-- property_descriptions -->
				<xs:element minOccurs="0" name="propertyDescription">
				  <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				  </xs:simpleType>
				</xs:element>
				<!-- review_count/review_sum gives star rating -->
                <xs:element minOccurs="0" name="review_count" type="xs:integer" />
				<!-- property_photos -->
                <xs:element minOccurs="0" name="property_photos">
