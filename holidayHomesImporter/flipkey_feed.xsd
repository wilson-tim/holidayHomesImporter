                </xs:element>
				<!-- property_fees -->
				<xs:element name="property_fees">
					<xs:complexType>
					  <xs:sequence>
						<xs:element minOccurs="0" maxOccurs="unbounded" name="property_fee" >
						<xs:complexType>
						  <xs:sequence>
							<xs:element name="fee_units" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
							<xs:element minOccurs="0" name="required" type="xs:integer" />
							<xs:element minOccurs="0" name="property_fee_type_id" type="xs:integer" />
							<xs:element name="name" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
							<xs:element name="cost" type="xs:integer" />
							<xs:element name="type" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
							<xs:element name="description" >
							  <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="4000"/>
								</xs:restriction>
							  </xs:simpleType>
							</xs:element>
						  </xs:sequence>
						</xs:complexType>
						</xs:element>
					  </xs:sequence>
					</xs:complexType>
				</xs:element>
                <xs:element minOccurs="0" name="property_videos" >
					<xs:complexType>
					  <xs:sequence>
						<xs:element minOccurs="0" maxOccurs="unbounded" name="property_video" >
							<xs:complexType>
								<xs:sequence>
									<xs:element name="baseurl" >
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="2000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="description" >
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="4000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="last_verified" type="xs:dateTime"/>
									<xs:element name="modified" type="xs:dateTime"/>
									<xs:element name="order" type="xs:integer"/>
									<xs:element name="status" >
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="video_source" >
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="video_source_id" >
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="video_source_updated" type="xs:dateTime"/>
									<xs:element name="video_thumb_filename" >
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					  </xs:sequence>
					</xs:complexType>
				</xs:element>
				<!-- property_default_rates -->
                <xs:element minOccurs="0" name="property_default_rates" >
					<xs:complexType>
					  <xs:sequence>
						<xs:element minOccurs="0" name="property_default_rate" >
							<xs:complexType>
							  <xs:sequence>
								<!-- These are rates to be used when there is no rate for a date range -->
								<xs:element name="modified" type="xs:dateTime"/>
								<xs:element name="minimum_length"  type="xs:integer"/>
								<xs:element name="day_max_rate" type="xs:decimal"/>
								<xs:element name="day_min_rate" type="xs:decimal"/>
								<xs:element name="month_max_rate" type="xs:decimal"/>
								<xs:element name="month_min_rate" type="xs:decimal"/>    
								<xs:element name="week_max_rate" type="xs:decimal"/>
								<xs:element name="week_min_rate" type="xs:decimal"/>
							  </xs:sequence>
							</xs:complexType>
						</xs:element>
					  </xs:sequence>
					</xs:complexType>
				</xs:element>
				<!-- review_photos was not in schema and only appears as empty element.. future functionality? -->
                <xs:element minOccurs="0" name="review_photos" type="xs:integer" />
				<!-- property_rate_summary -->
                <xs:element minOccurs="0" name="rateUser_day_min_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateDay_max_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateUser_week_min_rate"  type="xs:decimal" />
                <xs:element minOccurs="0" name="rateUser_month_max_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateWeek_max_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateUser_week_max_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateUser_month_min_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateMonth_max_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateMinimum_length" type="xs:integer" />
                <xs:element minOccurs="0" name="rateMonth_min_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateUser_day_max_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateDay_min_rate" type="xs:decimal" />
                <xs:element minOccurs="0" name="rateWeek_min_rate" type="xs:decimal" />
				<!-- property_rates -->
                <xs:element minOccurs="0" name="property_rates">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" maxOccurs="unbounded" name="property_rate">
                        <xs:complexType>
                          <xs:sequence>
                            <!-- <xs:choice maxOccurs="unbounded"> -->
                              <xs:element minOccurs="0" name="weekend_min_rate" type="xs:decimal" />
                              <xs:element minOccurs="0" name="weeknight_max_rate" type="xs:decimal" />
                              <xs:element minOccurs="0" name="end_date" type="xs:date" />
                              <xs:element minOccurs="0" name="week_max_rate" type="xs:decimal" />
                              <xs:element minOccurs="0" name="weekend_max_rate" type="xs:decimal" />
                              <xs:element minOccurs="0" name="start_date" type="xs:date" />
                              <xs:element minOccurs="0" name="label" >
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="255"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
                              <xs:element minOccurs="0" name="weekend_nights" >
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="255"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
                              <xs:element minOccurs="0" name="per_person_per_stay"  type="xs:decimal" />
                              <xs:element minOccurs="0" name="per_person_per_night" type="xs:decimal" />
                              <xs:element minOccurs="0" name="minimum_length" type="xs:integer" />
							  <!-- 0-6, 0= Sunday, the day that a stay must start/end, not used very much -->
                              <xs:element minOccurs="0" name="turn_day" >
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="255"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
                              <xs:element minOccurs="0" name="month_max_rate" type="xs:decimal" />
                              <xs:element minOccurs="0" name="per_person_over_amount" type="xs:decimal" />
